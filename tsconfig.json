{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2016",
    "module": "es6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/*.ts", "src/*.tsx"],
  "exclude": ["src/*.test.ts"]
}
